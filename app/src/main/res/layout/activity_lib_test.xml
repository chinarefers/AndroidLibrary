<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:easemob="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.trilink.androidlib.widget.EaseTitleBar
        android:id="@+id/title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        easemob:titleBarLeftImage="@drawable/ease_mm_title_back"
        easemob:titleBarTitle="libTest"

        easemob:titleBarBackground="#000000"
        easemob:titleBarRightImage="@drawable/ease_mm_title_back"
        >

    </com.trilink.androidlib.widget.EaseTitleBar>

    <com.trilink.androidlib.widget.EaseSwitchButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/switch_button">

    </com.trilink.androidlib.widget.EaseSwitchButton>
    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        >
        <com.trilink.androidlib.widget.photoview.EasePhotoView

            android:id="@+id/ease_imageview"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
</LinearLayout>